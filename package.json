{
  "name": "flag-game",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:network": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "docker:build": "docker build -t flag-game .",
    "docker:run": "docker run -p 8080:80 flag-game",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:components": "jest --config=jest.config.js --testPathPattern=src/app/components/**/*.spec.ts",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "format": "prettier . --write",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.9",
    "@angular/common": "^18.2.9",
    "@angular/compiler": "^18.2.9",
    "@angular/core": "^18.2.9",
    "@angular/forms": "^18.2.9",
    "@angular/platform-browser": "^18.2.9",
    "@angular/platform-browser-dynamic": "^18.2.9",
    "@angular/router": "^18.2.9",
    "@angular/service-worker": "^18.2.9",
    "firebase": "^11.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.0",
    "@testing-library/angular": "^17.3.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "angular-eslint": "18.4.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.16.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "prettier": "3.3.3",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.5.2",
    "typescript-eslint": "8.10.0"
  }
}
